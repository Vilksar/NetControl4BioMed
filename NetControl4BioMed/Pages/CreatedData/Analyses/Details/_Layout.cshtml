@{
    Layout = "/Pages/CreatedData/Analyses/_Layout.cshtml";
}

@section Banner {
    @RenderSection("Banner", required: false)
}

@section Breadcrumbs {
    <partial name="_NavigationBreadcrumbPartial" model="@Helpers.ViewModels.NavigationBreadcrumbViewModel.GetCreatedDataAnalysesDetailsNavigationBreadcrumb(Model.View.Analysis.Id)" />
    @RenderSection("Breadcrumbs", required: false)
}

@section Navigation {
    <partial name="_NavigationAreaPartial" model="@Helpers.ViewModels.NavigationAreaViewModel.GetCreatedDataAnalysesDetailsNavigationArea(Model.View.Analysis.Id)" />
    @RenderSection("Navigation", required: false)
}

@RenderBody()

@section Scripts {
    @RenderSection("Scripts", required: false)
}
