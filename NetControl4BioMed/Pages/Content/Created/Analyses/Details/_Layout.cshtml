@{
    Layout = "/Pages/Content/Created/Analyses/_Layout.cshtml";
}

@section Banner {
    @*<partial name="_NavigationBannerPartial" model="@Helpers.ViewModels.NavigationBannerViewModel.AnalysisNavigationBanner" />*@
    @RenderSection("Banner", required: false)
}

@section Breadcrumbs {
    <partial name="_NavigationBreadcrumbPartial" model="@Helpers.ViewModels.NavigationBreadcrumbViewModel.AnalysisDetailsNavigationBreadcrumb" />
    @RenderSection("Breadcrumbs", required: false)
}

@section Navigation {
    <partial name="_NavigationAreaPartial" model="@Helpers.ViewModels.NavigationAreaViewModel.GetAnalysisNavigationArea(Model.View.Analysis)" />
    @RenderSection("Navigation", required: false)
}

@RenderBody()

@section Scripts {
    @RenderSection("Scripts", required: false)
}
