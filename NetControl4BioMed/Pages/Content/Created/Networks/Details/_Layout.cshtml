@{
    Layout = "/Pages/Content/Created/Networks/_Layout.cshtml";
}

@section Banner {
    @*<partial name="_NavigationBannerPartial" model="@Helpers.ViewModels.NavigationBannerViewModel.NetworkNavigationBanner" />*@
    @RenderSection("Banner", required: false)
}

@section Breadcrumbs {
    <partial name="_NavigationBreadcrumbPartial" model="@Helpers.ViewModels.NavigationBreadcrumbViewModel.NetworkDetailsNavigationBreadcrumb" />
    @RenderSection("Breadcrumbs", required: false)
}

@section Navigation {
    <partial name="_NavigationAreaPartial" model="@Helpers.ViewModels.NavigationAreaViewModel.GetNetworkNavigationArea(Model.View.Network)" />
    @RenderSection("Navigation", required: false)
}

@RenderBody()

@section Scripts {
    @RenderSection("Scripts", required: false)
}
